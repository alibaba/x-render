    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "dayjs": "^1.11.7",
    "form-render": "^2.1.26",
    "lodash.get": "^4.4.2",
    "lodash.omit": "^4.4.2",
    "lodash.defaults": "^4.2.0",
    "zustand": "^4.1.5",
    "classnames": "^2.3.1",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "ahooks": "^3.7.5"
