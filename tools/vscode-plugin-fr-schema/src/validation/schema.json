{
  "title": "JSON schema for FormRender",
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "required": [
    "properties"
  ],
  "properties": {
    "type": {
      "type": "string",
      "const": "object",
      "default": "object",
      "description": "根节点类型，仅支持对象。"
    },
    "properties": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "string",
              "number",
              "boolean",
              "array",
              "object",
              "range",
              "html"
            ],
            "description": "type 描述里组件的值的数据类型。用于校验数据类型，也用于判断使用哪个组件来渲染，以及校验表单数据。"
          }
        }
      }
    }
  }
}
